<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="162.0" prefWidth="359.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.exa.ui.Window2.view.Window2Controller">
   <center>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" fillWidth="false" prefHeight="258.0" prefWidth="229.0" spacing="20.0">
               <children>
                  <HBox>
                     <children>
                        <TextField id="direccionArchDocentes" fx:id="direccionArchCatedras" promptText="Archivo Catedra" />
                        <Button mnemonicParsing="false" onAction="#examinarArchCatedras" text="Examinar" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <TextField id="direccionArchCatedras" fx:id="direccionArchDocentes" promptText="Archivo Docente" />
                        <Button mnemonicParsing="false" onAction="#examinarArchDocentes" text="Examinar" />
                     </children>
                  </HBox>
                  <HBox spacing="20.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#aceptar" prefHeight="34.0" prefWidth="69.0" text="Aceptar" />
                        <Button fx:id="cancelar" mnemonicParsing="false" onAction="#cancelar" prefHeight="34.0" prefWidth="62.0" text="Cancelar" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <ImageView fx:id="visualizadorDeImagen" fitHeight="93.0" fitWidth="104.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </HBox>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
